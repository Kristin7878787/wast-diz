{
  "name": "wast-diz",
  "version": "0.1.0",
  "description": "WebAssembly dizassembler",
  "main": "lib",
  "bin": {
    "wast-diz": "bin/diz.js"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "git submodule init && git submodule update --remote && git submodule status && istanbul cover _mocha test",
    "wasm-as": "for f in testsuite/*.wast; do ../binaryen/bin/wasm-as $f > wasm/$(basename ${f} .${f##*.}).wasm; done"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drom/wast-diz.git"
  },
  "keywords": [
    "WAST",
    "WebAssembly"
  ],
  "author": "Aliaksei Chapyzhenka",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/drom/wast-diz/issues"
  },
  "homepage": "https://github.com/drom/wast-diz#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^2.11.1",
    "istanbul": "^0.4.3",
    "mocha": "^2.5.3"
  }
}
